{
  "name": "@mozaik/ext-gitlab",
  "version": "2.0.0-alpha.0",
  "description": "Mozaik GitLab widgets",
  "repository": {
    "type": "git",
    "url": "git://github.com/plouc/mozaik-ext-gitlab"
  },
  "license": "MIT",
  "author": {
    "name": "RaphaÃ«l Benitte",
    "url": "https://github.com/plouc"
  },
  "homepage": "https://github.com/plouc/mozaik-ext-gitlab",
  "main": "./lib/components/index.js",
  "keywords": [
    "gitlab",
    "gitlab-ci",
    "mozaik",
    "widget",
    "extension",
    "dashboard"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "dependencies": {
    "bluebird": "3.3.4",
    "chalk": "1.1.3",
    "convict": "1.4.0",
    "lodash": "4.15.0",
    "moment": "2.14.1",
    "prop-types": "^15.5.10",
    "react-icons": "^2.2.5",
    "superagent": "2.2.0",
    "superagent-bluebird-promise": "3.0.2"
  },
  "devDependencies": {
    "@mozaik/ui": "^2.0.0-alpha.1",
    "ava": "0.16.0",
    "babel-cli": "^6.18.0",
    "babel-core": "6.13.2",
    "babel-eslint": "6.1.2",
    "babel-plugin-istanbul": "2.0.0",
    "babel-preset-react-app": "^2.0.1",
    "babel-register": "6.11.6",
    "coveralls": "2.11.12",
    "enzyme": "2.4.1",
    "eslint": "3.3.1",
    "eslint-plugin-react": "6.1.2",
    "mockery": "1.7.0",
    "nock": "7.7.2",
    "nyc": "8.1.0",
    "prettier": "^1.5.3",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.1",
    "react-dom": "^15.6.1",
    "sinon": "1.17.5"
  },
  "peerDependencies": {
    "@mozaik/ui": "^2.0.0-alpha.1",
    "nivo": "^1.0.0-alpha.7",
    "react": "^15.6.1"
  },
  "scripts": {
    "eslint": "eslint ./src/** ./test/**",
    "test": "ava",
    "test-cover": "nyc ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "cover-report": "nyc report --reporter=lcov && open coverage/lcov-report/index.html",
    "build": "rm -rf lib/* && babel src --no-comments --out-dir lib",
    "watch": "NODE_ENV=development babel src --watch --out-dir lib",
    "format": "prettier --tab-width=4 --bracket-spacing --no-semi --trailing-comma es5 --single-quote --write 'src/**/*.js' 'client.js'"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "tap": false,
    "failFast": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "all": true,
    "include": [
      "src"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
